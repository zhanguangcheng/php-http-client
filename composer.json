{
  "name": "zhanguangcheng/php-http-client",
  "description": "A simple and powerful HTTP client library for PHP.",
  "keywords": [
    "http-client",
    "curl"
  ],
  "type": "library",
  "version": "0.1.0",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/zhanguangcheng/php-http-client/issues?state=open",
    "source": "https://github.com/zhanguangcheng/php-http-client"
  },
  "require": {
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "Zane\\HttpClient\\": "src/"
    }
  }
}
